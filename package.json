{
  "name": "better-skills",
  "version": "1.0.1",
  "description": "Bidirectional converter between .agents/skills/ and .claude/skills/ formats",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "better-skills": "./src/index.ts"
  },
  "files": [
    "src/**/*.ts"
  ],
  "scripts": {
    "start": "bun src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "bump": "bumpp",
    "release": "bun run test && bun run lint && bumpp && npm publish"
  },
  "dependencies": {
    "effect": "^3.0.0",
    "@effect/cli": "^0.36.0",
    "@effect/platform": "^0.58.0",
    "yaml": "^2.4.0",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "bumpp": "^10.4.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "claude",
    "skills",
    "agents",
    "converter",
    "cli",
    "cursor",
    "copilot",
    "ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marve10s/Better-skills"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/Marve10s/Better-skills/issues"
  },
  "homepage": "https://github.com/Marve10s/Better-skills#readme"
}
